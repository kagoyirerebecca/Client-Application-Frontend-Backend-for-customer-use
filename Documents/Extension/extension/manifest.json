{
    "manifest_version": 2,
    "name": "Extension",
    "version": "1.0",
    "description": "Extension",
    "icons": {
      "48": "ECO Forest Icon G.png",
      "128": "ECO Forest Icon G.png"
    },
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_icon": "icons/icon48.png",
      "default_popup": "popup.html",
      "default_title": "Map Popup"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ]
  }
  